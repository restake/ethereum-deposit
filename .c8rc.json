{
    "all": true,
    "check-coverage": true,
    "include": [
        "src/**/!(*.test.*).[tj]s?(x)"
    ],
    "exclude": [
        "src/tests/**/*.*",
        "src/index.ts",
        "src/types.ts"
    ],
    "reporter": [
        "html",
        "lcov",
        "text",
        "text-summary"
    ],
    "report-dir": "coverage"
}
